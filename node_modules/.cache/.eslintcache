es, setModules] = useState([]);\n  const [loading, setLoading] = useState(true);\n\n\n\n  useEffect(() => {\n    const fetchModules = async () => {\n      try {\n        setLoading(true);\n        const fetchedModules = await getModulesByCourseId(courseId);\n        setModules(fetchedModules);\n      } catch (error) {\n        console.error('Error fetching modules:', error);\n      } finally {\n        setLoading(false);\n      }\n    };\n\n    fetchModules();\n  }, [courseId]);\n\n  const handleModuleAddition = (newModule) => {\n    setModules([...modules, newModule]);\n  };\n\n  if (loading) {\n    return <div>Loading...</div>;\n  }\n\n  return (\n    <div>\n      <h1>Modules for Course {courseId}</h1>\n      {/* <ModuleForm onModuleAddition={handleModuleAddition} courseId={courseId} /> */}\n      \n      <ModuleList modules={modules} courseId={courseId} />\n    </div>\n  );\n};\n\nexport default CourseModulesPage;\n","/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/HomePage.js",["180","181","182"],[],"import React, { useState, useEffect } from 'react';\nimport { useAuth } from '../contexts/AuthContext';\nimport CourseCard from '../components/Courses/CourseCard'; // 确保路径正确\nimport SearchBar from '../components/SearchBar.js';\nimport TitleSearchBar from '../components/TitleSearchBar'; // 引入 TitleSearchBar 组件\n\nimport { getEnrolledCourses } from '../api/course'; // 引入 getEnrolledCourses 函数\nimport './HomePage.css'; // 引入 HomePage.css 文件\n\nconst HomePage = () => {\n  const [enrolledCourses, setEnrolledCourses] = useState([]);\n  const { user } = useAuth(); // 使用 useAuth 钩子获取用户数据\n\n  useEffect(() => {\n    if (user) {\n      fetchEnrolledCourses();\n    }\n  }, [user]);\n\n  const username = user?.username; // 直接访问用户角色\n\n  const fetchEnrolledCourses = async () => {\n    try {\n      const courses = await getEnrolledCourses(user._id);\n      setEnrolledCourses(courses);\n    } catch (error) {\n      console.error('Error fetching enrolled courses:', error);\n    }\n  };\n\n  const allUsersInfo = (\n    <div>\n      <h2>3 Expert Tips For The Day</h2>\n      <blockquote>\n        \"Always control the center. It's the key to winning the game.\"\n        <footer>- National Master Ricky Wu</footer>\n      </blockquote>\n      <blockquote>\n        \"Don't rush. Take your time and think through your moves.\"\n        <footer>- National Master Sibo Wu</footer>\n      </blockquote>\n      <blockquote>\n        \"Study your endgames. It's the most important part of the game.\"\n        <footer>- National Master Sibo (Ricky) Wu</footer>\n      </blockquote>\n    </div>\n  );\n\n  // Hardcoded information for logged-in users\n  const loggedInInfo = (\n    <div>\n      <h2>Upcoming Online Events</h2>\n      <p>As a registered member, you have exclusive access to special online events.</p>\n    \n      <div className=\"event-card\">\n        <h4>Chess Strategies Webinar</h4>\n        <div className=\"event-details\">\n          <span className=\"event-date\">Date: December 15, 2023</span>\n        </div>\n        <div className=\"event-details\">\n          <span className=\"event-time\">Time: 13:00 - 14:30 PM EST</span>\n        </div>\n        <p>Join National Master Ricky Wu as he discusses advanced chess strategies. RSVP <a href=\"#\">here</a>.</p>\n      </div>\n      <div className=\"event-card\">\n        <h4>Beginner's Chess Workshop</h4>\n        <div className=\"event-details\">\n          <span className=\"event-date\">Date: December 28, 2023</span>\n        </div>\n        <div className=\"event-details\">\n          <span className=\"event-time\">Time: 13:00 - 14:30 PM EST</span>\n        </div>\n        <p>Learn the basics of chess with Grandmaster Jane Smith. RSVP <a href=\"#\">here</a>.</p>\n      </div>\n    </div>\n  );\n  \n  \n  return (\n    <div className=\"homepage-container\">\n\n      <div className=\"main-column\">\n        <div className=\"homepage-header\">\n          <h1>Hello {username }, Welcome to Chess Online Education</h1>\n          <p>Learn chess with the best courses and instructors online.</p>\n        </div>\n\n        <div className=\"expert-tips\">\n          {allUsersInfo}\n        </div>\n\n        {user && (\n          <div className=\"event-info\">\n            {loggedInInfo}\n          </div>\n        )}\n\n        {user && enrolledCourses.length > 0 && (\n          <div className=\"course-container\">\n            <h2>Continue Study</h2>\n            {enrolledCourses.map(course => (\n              <CourseCard \n                key={course._id} \n                course={course} \n                userRole={user.role} \n                isEnrolled={true} \n              />\n            ))}\n          </div>\n        )}\n      </div>\n      {/* Search column */}\n      <div className=\"search-column\">\n        <h1>Search Players</h1> {/* Title for the search bars */}\n        <p> Search by Usernames or Titles</p>\n        <div className=\"search-bar\">\n          <SearchBar />\n        </div>\n        <div className=\"title-search-bar\">\n          <TitleSearchBar />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default HomePage;\n","/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/RegisterPage.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/LoginPage.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/Profile.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/contexts/AuthContext.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/AdminPage.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/CoursesPage.js",["183","184"],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/components/Navbar.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/CartPage.js",["185","186","187"],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/BlogPage.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/TitlePlayersPage.js",["188"],[],"import React, { useEffect, useState } from 'react';\nimport { useParams, Link, useNavigate } from 'react-router-dom';\nimport { getPlayersByTitle } from '../api/chessApi';\nimport TitleSearchBar from '../components/TitleSearchBar'; // 引入 TitleSearchBar 组件\nconst TitlePlayersPage = () => {\n    const { title } = useParams();\n    const [players, setPlayers] = useState([]);\n    const navigate = useNavigate();\n\n    useEffect(() => {\n        // 尝试从 sessionStorage 获取保存的搜索结果\n        const savedPlayers = sessionStorage.getItem(`players-${title}`);\n        if (savedPlayers) {\n            setPlayers(JSON.parse(savedPlayers));\n        } else {\n            fetchPlayers();\n        }\n    }, [title]);\n\n    const fetchPlayers = async () => {\n        try {\n            const playerUsernames = await getPlayersByTitle(title);\n            setPlayers(playerUsernames);\n            // 将搜索结果保存到 sessionStorage\n            sessionStorage.setItem(`players-${title}`, JSON.stringify(playerUsernames));\n        } catch (error) {\n            console.error('Error fetching players by title:', error);\n        }\n    };\n\n    const handleTitleChange = (newTitle) => {\n        // 在新搜索时清除之前的保存结果\n        sessionStorage.removeItem(`players-${title}`);\n        navigate(`/title/${newTitle}`);\n    };\n\n    return (\n        <div>\n            <TitleSearchBar onTitleChange={handleTitleChange} />\n            <h1>{title} Players</h1>\n            <ul>\n                {players.map((username) => (\n                    <li key={username}>\n                        <Link to={`/player/${username}`}>{username}</Link>\n                    </li>\n                ))}\n            </ul>\n        </div>\n    );\n};\n\nexport default TitlePlayersPage;\n","/Users/levi/Desktop/chess/online-chess-react-platform/src/pages/DetailPage.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/components/