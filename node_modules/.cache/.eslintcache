w error;\n    }\n  };\n\n  // 登出用户\nexport const logoutUser = () => {\n  // 移除存储的用户信息和 token\n  localStorage.removeItem('user');\n  localStorage.removeItem('userId');\n};\n","/Users/levi/Desktop/chess/online-chess-react-platform/src/components/Courses/CourseCard.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/api/chessApi.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/components/PlayerProfile.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/components/Courses/CourseForm.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/components/Modules/ModuleCard.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/api/video.js",[],[],"/Users/levi/Desktop/chess/online-chess-react-platform/src/components/VideoPlayer.js",[],[],{"ruleId":"190","replacedBy":"191"},{"ruleId":"192","replacedBy":"193"},{"ruleId":"194","replacedBy":"195"},{"ruleId":"196","replacedBy":"197"},{"ruleId":"198","replacedBy":"199"},{"ruleId":"200","replacedBy":"201"},{"ruleId":"202","severity":1,"message":"203","line":4,"column":8,"nodeType":"204","messageId":"205","endLine":4,"endColumn":18},{"ruleId":"202","severity":1,"message":"206","line":30,"column":9,"nodeType":"204","messageId":"205","endLine":30,"endColumn":29},{"ruleId":"207","severity":1,"message":"208","line":18,"column":6,"nodeType":"209","endLine":18,"endColumn":12,"suggestions":"210"},{"ruleId":"211","severity":1,"message":"212","line":63,"column":90,"nodeType":"213","endLine":63,"endColumn":102},{"ruleId":"211","severity":1,"message":"212","line":73,"column":72,"nodeType":"213","endLine":73,"endColumn":84},{"ruleId":"202","severity":1,"message":"214","line":44,"column":9,"nodeType":"204","messageId":"205","endLine":44,"endColumn":24},{"ruleId":"202","severity":1,"message":"215","line":53,"column":9,"nodeType":"204","messageId":"205","endLine":53,"endColumn":27},{"ruleId":"202","severity":1,"message":"216","line":6,"column":8,"nodeType":"204","messag